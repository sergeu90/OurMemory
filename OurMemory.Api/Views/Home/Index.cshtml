<div>
    <input type="file" multiple name="upload" id="uploadFile" /><br />
    <button id="submit">Загрузить</button>
</div>



<script type="text/javascript">

    $('#submit').on('click', function (e) {
        e.preventDefault();
        var files = document.getElementById('uploadFile').files;
        if (files.length > 0) {
            if (window.FormData !== undefined) {
                var data = new FormData();
                for (var x = 0; x < files.length; x++) {
                    data.append("file" + x, files[x]);
                }

                data.append("awards", "1");
                data.append("birthPlace", "2");
                data.append("called", "2015-12-08T21:00:00.000Z");
                data.append("dateBirth", "2015-12-08T21:00:00.000Z");
                data.append("dateDeath", "2015-12-08T21:00:00.000Z");
                data.append("description", "2");
                data.append("firstName", "2");
                data.append("lastName", "2");
                data.append("latitude", "2");
                data.append("longitude", "2");
                data.append("middleName", "2");
                data.append("troops", "2");

                $.ajax({
                    type: "POST",
                    url: '/api/veteran',
                    contentType:false,
                    processData: false,
                    data:  data ,
                    success: function (result) {
                        alert(result);
                    },
                    error: function (xhr, status, p3) {
                        alert(xhr.responseText);
                    }
                });
            } else {
                alert("Браузер не поддерживает загрузку файлов HTML5!");
            }
        }
    });
</script>